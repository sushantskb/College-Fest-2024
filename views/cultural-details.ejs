<link rel="stylesheet" href="/css/event.css">
<style>
    .anime {
        filter: drop-shadow(6px 6px 6px black);
        position: relative;
        animation: mymove 1.5s infinite;
    }

    @keyframes mymove {
        33% {
            top: 0px;
        }

        66% {
            top: 20px;
        }

        100% {
            top: 0px;
        }
    }
</style>
<div id="page1" style="background-repeat: no-repeat; background-size: cover;">
        <nav>
            <a href="/"><img
                class="magnet"
                src="/assets/img/Shrujan final1.png"
                width="160"
                alt="shrujan logo"
              />
            </a>
            <div id="nav-part2">
              <h4><a class="magnet" href="/">Home</a></h4>
              <h4><a class="magnet" href="#">Team</a></h4>
              <h4><a class="magnet" href="#">Contact</a></h4>
            </div>
            <h3 style="color: #fff; filter: drop-shadow(1px 1px 1px black);"><i class="ri-menu-fill" style="padding-right: 10px;"></i></i>Menu</h3>
        </nav>
        <% if(infoErrorObj != "") { %>
            <div class="failure-message">
              <%= infoErrorObj %>
            </div>
        <% } %>

        <div id="page3">

            

        <% if(eventsData != "" || eventsData != "undefined" || eventsData != []) { %>
            <%  eventsData.forEach(function (event, index){ %>
                    <div class="child anime" id="child1">
                        <img src="/assets/img/cultural/<%= event.poster %>" style="filter: drop-shadow(5px 5px #fff); background-color: rgba(255, 255, 255, 0.075); background-color: rgba(255, 255, 255, 0.20); border-radius: 30px;
                        border: 2px solid pink;"
                            alt="" loading="lazy">
                        <div class="actions">
                            <a href="/events/cultural/<%= type %>/<%= event.ename %>"><%= event.name %></a>
                            <a href="/culturalDetails/<%= event.details %>" download style="background: none; border: none; box-shadow: none;"><button>Details</button></a>
                        </div>
                    </div>
            <% }) %>
        <% } else { %>
            <h1>Comming Soon</h1>
        <% } %>

    </div>

        
</div>